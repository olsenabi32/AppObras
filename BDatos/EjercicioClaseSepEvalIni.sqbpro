ow_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="78"/><column index="2" value="62"/><column index="3" value="85"/><column index="4" value="70"/><column index="5" value="36"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">SELECT 
    T.NOMBRE, 
    S.COD_OBRA, 
    T.PVP_HORA AS &quot;PVP HORA&quot;, 
    O.DESCRIPCION,
    
    -- Formateo de Hora Entrada (HH:MI)
    SUBSTR(S.HORA_ENT, 1, 2) || ':' || SUBSTR(S.HORA_ENT, 3, 2) AS &quot;HORA INICIO&quot;,
    
    -- Formateo de Hora Salida (HH:MI)
    SUBSTR(S.HORA_SAL, 1, 2) || ':' || SUBSTR(S.HORA_SAL, 3, 2) AS &quot;HORA FIN&quot;,
    
    -- Formateo de Fecha (DD-MM-YYYY)
    SUBSTR(S.FECHA, 1, 2) || '-' || SUBSTR(S.FECHA, 3, 2) || '-' || SUBSTR(S.FECHA, 5, 4) AS FECHA,
    
    -- 1. Cálculo del Total de Horas
    ROUND(CAST(
        (
            (CAST(SUBSTR(S.HORA_SAL, 1, 2) AS INTEGER) * 60 + CAST(SUBSTR(S.HORA_SAL, 3, 2) AS INTEGER))
            -
            (CAST(SUBSTR(S.HORA_ENT, 1, 2) AS INTEGER) * 60 + CAST(SUBSTR(S.HORA_ENT, 3, 2) AS INTEGER))
        ) / 60.0
        AS REAL
    ), 2) AS &quot;TOTAL HORAS&quot;,
    
    -- 2. Cálculo del Coste Total
    ROUND(
        (
            CAST(
                (
                    (CAST(SUBSTR(S.HORA_SAL, 1, 2) AS INTEGER) * 60 + CAST(SUBSTR(S.HORA_SAL, 3, 2) AS INTEGER))
                    -
                    (CAST(SUBSTR(S.HORA_ENT, 1, 2) AS INTEGER) * 60 + CAST(SUBSTR(S.HORA_ENT, 3, 2) AS INTEGER))
                ) / 60.0
                AS REAL
            )
        ) * T.PVP_HORA,
        2
    ) AS &quot;COSTE TOTAL&quot;
    
FROM
    SERVICIOS S
JOIN
    TRABAJADORES T ON S.NIF_TRAB = T.NIF
JOIN
    OBRAS O ON S.COD_OBRA = O.COD_OBRA

WHERE 
    T.NIF = '12345678A'  -- ¡CORREGIDO! Usando el alias T.NIF
    AND S.FECHA
    BETWEEN '20251021' AND '20251023' 
    
ORDER BY T.NOMBRE, S.COD_OBRA, S.FECHA, S.HORA_ENT;</sql><current_tab id="0"/></tab_sql></sqlb_project>
